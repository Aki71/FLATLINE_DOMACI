@model Vezba

<h2 style="padding-top:70px;">Detalji za @Model.VezbaName</h2>
<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="VezbaId"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.VezbaId
        </dd>
        <dt class="col-sm-2">
            <label asp-for="VezbaName"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.VezbaName
        </dd>
        <dt class="col-sm-2">
            <label asp-for="VezbaDescription"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.VezbaDescription
        </dd>
        <dt class="col-sm-2">
            <label asp-for="Rekviziti"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.Rekviziti
        </dd>
        <dt class="col-sm-2">
            <label asp-for="Trening.Name"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.Trening.Name
        </dd>

    </dl>
</div>
<div>
    <a asp-controller="Vezba" asp-action="Index">Nazad</a>
</div>